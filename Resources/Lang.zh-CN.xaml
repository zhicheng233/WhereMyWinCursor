<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <sys:String x:Key="Setting">设置</sys:String>
    <sys:String x:Key="Appearance">外观</sys:String>
    <sys:String x:Key="App-Theme">应用主题</sys:String>
    <sys:String x:Key="SelectWhichAppThemeToDisplay">选择要显示的主题</sys:String>

    <sys:String x:Key="Language">语言</sys:String>
    <sys:String x:Key="SelectWhichLanguageToDisplay">选择要显示的语言</sys:String>

    <sys:String x:Key="Behavior">行为</sys:String>
    <sys:String x:Key="StartWithComputerLaunch">开机自启</sys:String>
    <sys:String x:Key="StartTheAppWhenTheComputerHasLaunched">在电脑开机后自动启动此应用</sys:String>

    <sys:String x:Key="Cursor">光标</sys:String>
    <sys:String x:Key="InitialSize">初始大小</sys:String>
    <sys:String x:Key="TheInitialCursorSizeDefaultsTo32">设置初始光标大小，默认为 32</sys:String>
    <sys:String x:Key="MaxSize">最大大小</sys:String>
    <sys:String x:Key="TheMaxCursorSizeDefaultsTo128">设置放大后最大光标大小，默认为 128</sys:String>

    <sys:String x:Key="Animation">动画</sys:String>
    <sys:String x:Key="AStep">步进</sys:String>
    <sys:String x:Key="AnimationStep">动画步进</sys:String>
    <sys:String x:Key="Delay">延迟</sys:String>
    <sys:String x:Key="TheDelayBetweenAnimationStep">动画步进之间的延迟（ms）；如果值等于 0，则使用“Task.Yield()”</sys:String>

    <sys:String x:Key="PauseTime">维持时间</sys:String>
    <sys:String x:Key="PauseTimeForRestore">恢复到原始大小的时间(ms)</sys:String>

    <sys:String x:Key="Tray_Open_Setting">打开设置</sys:String>
    <sys:String x:Key="Tray_Exit">退出</sys:String>

    <sys:String x:Key="About">关于</sys:String>
    <sys:String x:Key="Version">版本</sys:String>
    <sys:String x:Key="AboutDescription">一个仿MAC的查找鼠标工具</sys:String>
    <sys:String x:Key="AboutOpenTheRepositories">在Github打开本项目</sys:String>
</ResourceDictionary>